[[provider]]
provider = "github"
name = "{{ .github.user }}"
path = "github"
env_var = "GITHUB_TOKEN"
skip_forks = false
exclude = [
    "pypi-data/pypi-mirror-.*",
    "pypi-data/pypi-json-data",
]

[[provider]]
provider = "github"
name = "pypi-data"
path = "github"
env_var = "GITHUB_TOKEN"
skip_forks = false
exclude = [
    "pypi-data/pypi-mirror-.*",
    "pypi-data/pypi-json-data",
]
