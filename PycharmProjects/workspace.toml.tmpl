[[provider]]
provider = "github"
name = "{{ .github.user }}"
path = "github"
env_var = "GITHUB_TOKEN"
skip_forks = false
exclude = [
    "pypi-data/.*"
]

[[provider]]
provider = "gitlab"
name = "{{ .gitlab.user }}"
url = "https://gitlab.com"
path = "gitlab"
env_var = "GITLAB_TOKEN"

[[provider]]
provider = "github"
name = "pypi-data"
path = "github"
env_var = "GITHUB_TOKEN"
skip_forks = false
exclude = [
    "pypi-data/pypi-code-.*"
]
